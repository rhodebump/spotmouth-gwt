<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:smc='urn:import:com.spotmouth.gwt.client.common'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>




  <g:HTMLPanel>


<div id="contact">

<h1>Contact Us!</h1>
<label>
<p>Your Name</p>
    <!--
<input type="text" required>
-->
    <smc:TextField required="true" ui:field="yourNameTextBox"/>
</label>

<label>
<p>E-mail</p>
    <!--
<input type="email" placeholder="example@mail.com" required>
can't do type=email here for IE
-->
    <smc:TextField placeholder="example@mail.com" required="true" ui:field="email"/> </label>

<label>
<p>Message Subject</p>
    <g:TextBox ui:field="messageSubject"/>
</label>

<label>
<p>Your Message</p>
<g:TextArea ui:field="contentTextArea"/>
</label>

<g:Button ui:field="sendButton"/>
</div>



</g:HTMLPanel>

</ui:UiBinder>