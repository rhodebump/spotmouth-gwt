<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:sm="urn:import:com.spotmouth.gwt.client">
<g:HTMLPanel>


    <div style="display:none">
        <span ui:field='friendCountSpan'/>



    </div>

    <div id="content" class="content">

   		<div class="friends _page">
   			<form id="f-search" class="search">
   				<label style="width: 92%;"><input type="text" placeholder="Find Friends"/>
   				<button><span class="_icon _search"/></button></label>
   			</form>
   			<form id="i-search" class="search" style="display:none;">
   				<label style="width: 92%;"><input type="text" placeholder="Find Friends"/>
   				<button><span class="_icon _search"/></button></label>
   			</form>
   			<h1 id="invite">
                   <g:Button  styleName="_white" ui:field="inviteFriendButton">Invite Friends</g:Button></h1>
   			<ul id="fr-tab-nav" class="_tab_navigation _top _two">
   				<li class="_active"><button>Friends</button></li>
   				<li><button>Invited</button></li>
   			</ul>
   			<ul id="fr-tab" class="_tabs">
   				<li class="_active">
   					<div class="content">

                           <sm:ULPanel addStyleNames="_list friends" ui:field="friendsULPanel"/>


   					</div>
   				</li>
   				<li>
   					<div class="content">
   						<div class="_list invited">
   							<a href="javascript:;">
   								<p><span class="_icon _inbox_acc"></span>Email@example.com</p>
   								<p><span class="_icon _phone_acc"></span>0-000-00-00</p>
   							</a>
   							<a href="javascript:;">
   								<p class="_sent"><span class="_icon _inbox_acc"></span>Email@example.com</p>
   								<p class="_sent"><span class="_icon _phone_acc"></span>0-000-00-00</p>
   							</a>
   							<a href="javascript:;">
   								<p class="_acc"><span class="_icon _inbox_acc"></span>Email@example.com</p>
   								<p class="_acc"><span class="_icon _phone_acc"></span>0-000-00-00</p>
   							</a>
   						</div>
   					</div>
   				</li>
   			</ul>

   		</div>

   	</div>



</g:HTMLPanel>

</ui:UiBinder>
