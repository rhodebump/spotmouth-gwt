<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
        >


<g:HTMLPanel tag="li" styleName="cl_item">

    <g:Image ui:field="contestImage" styleName="cl_item_photo"/>


   		<div class="cl_item_center">
   			<h3><g:Anchor ui:field="detailAnchor"> <span ui:field='nameSpan'/></g:Anchor></h3>
   			<p><span ui:field='descriptionSpan'/></p>
   		</div>
   		<span class="cl_btns">
             <g:Button styleName="button" ui:field="mostVotedButton">Most Voted</g:Button>
               <g:Button styleName="button" ui:field="winnersButton">Winners</g:Button>

   		</span>




    </g:HTMLPanel>


</ui:UiBinder>