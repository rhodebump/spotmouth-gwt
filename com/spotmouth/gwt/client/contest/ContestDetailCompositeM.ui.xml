<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel>


        <div id="content" class="content">
       		<div class="contest_details _page">
                   <g:Button ui:field="cancelButton" styleName="_dark">Go Back</g:Button>

       			<h1><span class="_icon _thumb_down"></span><span class="stars _icon _star">x10</span> <span ui:field='nameSpan'/></h1>
       			<div class="_img_wrap">
                       <!--
       				<img src="css/img/contest.png" class="_m"/>
                                      -->
                       <g:InlineLabel ui:field="photoSpan" styleName="_m"/>
       			</div>
       			<div class="_button_bar _dark _big">

                       <g:Button ui:field="mostVotedButton">Most Voted</g:Button>



                       <g:Button styleName="edit" ui:field="editButton"><span class="_icon _mark _active"></span></g:Button>

                       <g:Button ui:field="winnersButton">Winners</g:Button>


       			</div>
       			<label><h3>Date</h3></label>
       			<label class="_date"><p>From: <span ui:field='startDateSpan'/></p></label>
       			<label><p>To: <span ui:field='endDateSpan'/></p></label>
       			<span class="_sep _light"></span>
       			<label><h3>Address</h3></label>
       			<label><p><span ui:field='locationSpan'/></p></label>
       			<span class="_sep _light"></span>
       			<label><h3>Description</h3></label>
       			<label><p><span ui:field='descriptionSpan'/></p></label>
       			<span class="_sep _light"></span>
       			<label><h3>Radius</h3></label>
       			<label><p><span ui:field='radiusSpan'/> mile</p></label>
       			<span class="edit _sep _light"></span>
       			<label class="stars">
       				<h3>Stars</h3>
       				<p><span class="_icon _star _inline" style="float: left;"></span>x<span ui:field='starCountSpan'/></p>
       			</label>
       			<span class="stars _sep _light"></span>

       		</div>
       	</div>

</g:HTMLPanel>

</ui:UiBinder>
