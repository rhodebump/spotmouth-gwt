<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel>



<div class="add_contest" id="add_contest">
       	<div class="content">
               <g:Button ui:field="cancelButton" styleName="_dark">Go Back</g:Button>
               <g:InlineLabel ui:field="photoSpan"/>

       		<h1 class="ac_title"><span ui:field='nameSpan'/><span class="ac_stars">x&nbsp;<span ui:field='starCountSpan'/></span></h1>
       		<h2 class="ac_title"><span ui:field='locationSpan'/><span class="ac_radius" title="Radius"><span ui:field='radiusSpan'/></span></h2>
       		<p class="ac_description"><span ui:field='descriptionSpan'/></p>
       		<div class="ac_date_block">
       			<span class="ac_date ac_date_from"><img src="css/date.png"/><span ui:field='startDateSpan'/></span>
       			<span class="ac_date ac_date_to"><img src="css/enddate.png"/><span ui:field='endDateSpan'/></span>
       		</div>
                          <div class="_btns_wrap">


                                       <g:Button styleName="button" ui:field="winnersButton">Winners</g:Button>
                              <g:Button styleName="button" ui:field="mostVotedButton">Most Voted</g:Button>
                          </div>
               <g:Button addStyleNames="button ac_edit" ui:field="editButton">Edit</g:Button>


       	</div>
       </div>


</g:HTMLPanel>

</ui:UiBinder>
