<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel>



<div class="add_contest" id="add_contest">
       	<div class="content">

               <g:InlineLabel styleName="ac_photo" ui:field="photoSpan"/>

       		<h1 class="ac_title"><span ui:field='nameSpan'/></h1>
       		<h2 class="ac_title"><span ui:field='locationSpan'/></h2>
       		<p class="ac_description"><span ui:field='descriptionSpan'/></p>
       		<div class="ac_date_block">
       			<span class="ac_date ac_date_from"><img src="css/date.png"/><span ui:field='startDateSpan'/></span>
       			<span class="ac_date ac_date_to"><img src="css/enddate.png"/><span ui:field='endDateSpan'/></span>
       		</div>
               <g:Button addStyleNames="button" ui:field="joinButton">Join Chat</g:Button>
               <g:Button addStyleNames="button ac_edit" ui:field="editButton">Edit</g:Button>


       	</div>
       </div>


</g:HTMLPanel>

</ui:UiBinder>