<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="myResources" type="com.spotmouth.gwt.client.icons.SpotImageResource"/>




  <g:HTMLPanel>



<div class="content">
    <div style="display:none">
        <span ui:field='labelSpan'/>
    </div>
    <!--
     <a href="#" id="goback" title="Back to Search Results">
     <img src="images/back.png"></img>
     </a>
     -->
    <g:Anchor title="Back to Search Results" ui:field="backToSearchResultsAnchor">  <img src="images/back.png"/></g:Anchor>
     <div class="photo">

         <g:Image ui:field="mainImage"/>

     <div class="partners">
     <span class="yelp"><a href="#"></a><a href="#"></a></span>
     <span class="forsq"><a href="#"></a></span>
     </div>
     </div>
     <div class="info">
         <h1><span ui:field='nameSpan'/></h1>

         <h4 class="address"><span ui:field='addressSpan'/></h4>

       <g:HTML ui:field="hoursHTML"/>
         <g:Anchor  addStyleNames="tab-tgl fw-tab-one" title="View Map" ui:field="mapAnchor">View Map</g:Anchor>

         <g:Anchor addStyleNames="tab-tgl fw-tab-two" title="Description" ui:field="descriptionAnchor">Description</g:Anchor>
         <g:Anchor addStyleNames="tab-tgl fw-tab-three" title="Groups" ui:field="groupsAnchor">Groups</g:Anchor>

         <div id="fw-desc">
             <span ui:field='descriptionSpan'/>
         </div>
         <div id="fw-group">
             <g:FlowPanel ui:field="groupsPanel"/>
             <g:Button ui:field="addGroupButton"/>

         </div>
         <div id="fw-map">
             <g:SimplePanel ui:field="mapPanel"/>
         </div>


     <ul class="btm-bar">


         <li><g:Anchor ui:field="phoneAnchor"></g:Anchor></li>

     <li class="mark">
         <g:Anchor title="Mark This Spot" ui:field="markSpotAnchor"></g:Anchor>

     </li>
     <li class="biz">
         <g:Anchor title="This is my biz" ui:field="addMyBizAnchor"></g:Anchor>

         </li>
     <li class="admin">
         <g:Anchor title="Administer Spot" ui:field="adminSpotAnchor"></g:Anchor>
         </li>
     <li class="addf">

         <g:Anchor title="Add to Favorite" ui:field="addtoFavoriteAnchor"></g:Anchor>

     </li>

     <li class="website">

         <g:Anchor title="Website" ui:field="websiteAnchor" target="_blank"></g:Anchor>

        </li>


     </ul>
     </div>
     </div>
     <div class="comments">
     </div>

</g:HTMLPanel>

</ui:UiBinder>
